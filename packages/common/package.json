{
  "name": "@rollem/common",
  "version": "0.0.3-alpha.0",
  "description": "Rollem for Discord utilities.",
  "author": "Lemtzas <lemtzas@gmail.com> (http://twitter.com/lemtzas)",
  "homepage": "https://github.com/lemtzas/rollem-discord",
  "bugs": {
    "url": "https://github.com/lemtzas/rollem-discord/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "typeorm": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js"
  },
  "prepublish": "tsc",
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "spec": "src/**/*.spec.ts",
    "extension": [
      "ts"
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@language-v1": "rollem-dist/rollem-language-1/",
    "@language-v2": "rollem-dist/rollem-language-2/",
    "@bot": "rollem-dist/rollem-bot",
    "@storage": "rollem-dist/storage/"
  },
  "dependencies": {
    "@discordjs/uws": "^10.149.0",
    "@types/chai": "^4.2.11",
    "@types/chance": "^1.1.0",
    "@types/keyv": "^3.1.1",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.10.2",
    "applicationinsights": "^1.0.2",
    "chai": "^3.5.0",
    "chance": "^1.1.5",
    "del": "^3.0.0",
    "discord.js": "12",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-pegjs": "^0.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-watch": "^4.3.11",
    "injection-js": "^2.2.1",
    "lodash": "^4.17.15",
    "mocha": "^3.2.0",
    "module-alias": "^2.2.0",
    "moment": "^2.22.1",
    "next-transpile-modules": "^6.3.0",
    "node-cache": "^5.1.0",
    "npm": "^6.3.0",
    "nyc": "^15.0.1",
    "pegjs": "0.10.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.9.1",
    "ts-pegjs": "^0.2.6",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "0.3.0-rc.21",
    "typescript": "^3.0.3",
    "watchify": "^3.9.0"
  },
  "license": "MIT",
  "devDependencies": {
    "next-cli": "^1.0.0"
  }
