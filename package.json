{
  "name": "rollem-discord",
  "version": "0.1.1",
  "description": "Rollem for Discord - A dicebot and parser that aggressively matches rolls, and supports a variety of roll syntax.",
  "author": "Lemtzas <lemtzas@gmail.com> (http://twitter.com/lemtzas)",
  "main": "rollem.js",
  "homepage": "https://github.com/lemtzas/rollem-discord",
  "bugs": {
    "url": "https://github.com/lemtzas/rollem-discord/issues"
  },
  "scripts": {
    "start": "node rollem-dist/rollem-bot/bot.js",
    "start-local-one": "node -r dotenv/config ./rollem-dist/rollem-bot/bot.js dotenv_config_path=./secrets/vscode.env",
    "start-local-many": "node -r dotenv/config ./rollem-dist/rollem-bot/sharder.js dotenv_config_path=./secrets/vscode.env",
    "start-package": "docker run --env-file ./secrets/vscode.env rollem-discord",
    "package": "docker build -t rollem-discord .",
    "test": "mocha",
    "build": "gulp build",
    "watch": "gulp watch",
    "terraform": ".\\infra\\terraform\\scripts\\xxx-bash-docker-windows-proxy.sh run -it -v `pwd`infra/terraform/clusters:/app/ -w /app/ hashicorp/terraform:light"
  },
  "files": [
    "./"
  ],
  "dependencies": {
    "@types/inversify": "^2.0.33",
    "@types/node": "^10.9.3",
    "applicationinsights": "^1.0.2",
    "discord.js": "^11.2.1",
    "inversify": "^5.0.1",
    "moment": "^2.22.1",
    "npm": "^6.3.0",
    "pegjs": "0.10.0"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-watch": "^4.3.11",
    "mocha": "^3.2.0",
    "typescript": "^3.0.3",
    "watchify": "^3.9.0"
  }
}
